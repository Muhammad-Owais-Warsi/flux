{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "opener:default",
    "fs:default",
    "fs:allow-app-write",
    "fs:allow-app-write-recursive",
    {
      "identifier": "fs:default",
      "allow": [{  "path": "$APPDATA/**/*" }]
    }
  ]
}